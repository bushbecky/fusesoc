SAPI=1
[main]
name = ordb2a
description = "ordb2a board OpenRISC system"

simulators =
 icarus
 modelsim

backend = quartus


[icarus]
iverilog_options = -DSIM

[modelsim]
vlog_options = +define+SIM

[quartus]
family = "Cyclone IV E"
device = EP4CE22F17C6
sdc_files = data/ordb2a-ep4ce22.sdc
tcl_files = data/pinmap.tcl

